                            <li data-value="hvr-icon-up"><a>Icon Up</a></li>
                            <li data-value="hvr-icon-spin"><a>Icon Spin</a></li>
                            <li data-value="hvr-icon-drop"><a>Icon Drop</a></li>
                            <li data-value="hvr-icon-fade"><a>Icon Fade</a></li>
                            <li data-value="hvr-icon-float-away"><a>Icon Float Away</a></li>
                            <li data-value="hvr-icon-sink-away"><a>Icon Sink Away</a></li>
                            <li data-value="hvr-icon-grow"><a>Icon Grow</a></li>
                            <li data-value="hvr-icon-shrink"><a>Icon Shrink</a></li>
                            <li data-value="hvr-icon-pulse"><a>Icon Pulse</a></li>
                            <li data-value="hvr-icon-pulse-grow"><a>Icon Pulse Grow</a></li>
                            <li data-value="hvr-icon-pulse-shrink"><a>Icon Pulse Shrink</a></li>
                            <li data-value="hvr-icon-push"><a>Icon Push</a></li>
                            <li data-value="hvr-icon-pop"><a>Icon Pop</a></li>
                            <li data-value="hvr-icon-bounce"><a>Icon Bounce</a></li>
                            <li data-value="hvr-icon-rotate"><a>Icon Rotate</a></li>
                            <li data-value="hvr-icon-grow-rotate"><a>Icon Grow Rotate</a></li>
                            <li data-value="hvr-icon-float"><a>Icon Float</a></li>
                            <li data-value="hvr-icon-sink"><a>Icon Sink</a></li>
                            <li data-value="hvr-icon-bob"><a>Icon Bob</a></li>
                            <li data-value="hvr-icon-hang"><a>Icon Hang</a></li>
                            <li data-value="hvr-icon-wobble-horizontal"><a>Icon Wobble Horizontal</a></li>
                            <li data-value="hvr-icon-wobble-vertical"><a>Icon Wobble Vertical</a></li>
                            <li data-value="hvr-icon-buzz"><a>Icon Buzz</a></li>
                            <li data-value="hvr-icon-buzz-out"><a>Icon Buzz Out</a></li>
                        </ul>
                    </li>
                    <li class="dropdown-submenu">
                        <!-- Border Transitions -->
                        <a tabindex="-1" href="#">Border Transitions</a>
                        <ul class="dropdown-menu">
                            <!-- Options list -->
                            <li data-value="hvr-border-fade"><a>Border Fade</a></li>
