<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Customize Snippets -->
        <template id="custom_snippets" inherit_id="website.snippets" name="Custom Snippets">
            <!-- Customize Structure -->
            <xpath expr="//div[@id='snippet_structure']" position="inside">
                <!-- Vertical slide -->
                <div id="vertical_slide">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/website/static/src/img/blocks/block_banner.png"/>
                        <span class="oe_snippet_thumbnail_title">Vertical Slide</span>
                    </div>
                    <div id="carousel_vertical" class="oe_snippet_body carousel slide vertical">
                        <!-- Indicators -->
                        <ol class="carousel-indicators hidden">
                            <li data-target="#carousel_vertical" data-slide-to="0" class="active"/>
                        </ol>
                        <!-- Carousel items -->
                        <div class="carousel-inner">
                            <div class="item active" style="background-image: url('/website/static/src/img/banner/color_splash.jpg')">
                                <div class="container">
                                    <div class="row content">
                                        <div class="carousel-content col-md-6 col-sm-12">
                                            <h2>Your Banner Title</h2>
                                            <h3>Click to customize this text</h3>
                                            <p>
                                                <a href="/page/website.contactus" class="btn btn-success btn-large">Contact us</a>
                                            </p>
                                        </div>
                                        <div class="carousel-img col-md-6 hidden-sm hidden-xs">
                                            <img class="img-responsive" src="/website/static/src/img/banner/banner_picture.png" alt="Banner Odoo Image"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- Carousel nav -->
                        <div class="carousel-control left hidden" data-target="#carousel_vertical" data-slide="prev" style="height: 100%"><i class="fa fa-chevron-up"/></div>
                        <div class="carousel-control right hidden" data-target="#carousel_vertical" data-slide="next" style="height: 100%"><i class="fa fa-chevron-down"/></div>
                    </div>
                </div><!-- End of vertical slide -->

                <!-- Banner block -->
                <div id="banner_block">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/website/static/src/img/aqua.jpg"/>
                        <span class="oe_snippet_thumbnail_title">Banner Block</span>
                    </div>
                    <section id="banner_block_section" class="oe_snippet_body" style="background-image: url('/website/static/src/img/banner/color_splash.jpg'); background-size: cover;">
                        <div id="banner_block_content" class="content">
                            <div class="container">
                                <div class="row">
                                    <a class="col-md-3 col-xs-6 mt16 mb16" href="#">
                                        <img class="img-responsive md16 img-circle hover" src="/website/static/src/img/parallax/parallax_bg.jpg" alt="Odoo Image"/>
                                    </a>
                                    <a class="col-md-3 col-xs-6 mt16 mb16" href="#">
                                        <img class="img-responsive md16 img-circle hover" src="/website/static/src/img/parallax/parallax_photo1.jpg" alt="Odoo Image"/>
                                    </a>
                                    <a class="col-md-3 col-xs-6 mt16 mb16" href="#">
                                        <img class="img-responsive md16 img-circle hover" src="/website/static/src/img/parallax/parallax_bg.jpg" alt="Odoo Image"/>
                                    </a>
                                    <a class="col-md-3 col-xs-6 mt16 mb16" href="#">
                                        <img class="img-responsive md16 img-circle hover" src="/website/static/src/img/parallax/parallax_photo1.jpg" alt="Odoo Image"/>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </section>
                </div><!-- End of banner block -->

            </xpath>

            <!-- Customize Content -->
            <xpath expr="//div[@id='snippet_content']" position="inside">
                <!-- Hover button -->
                <div id="hover_button">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/snippet_addons/static/src/img/block_button_hover.png"/>
                        <span class="oe_snippet_thumbnail_title">Hover Button</span>
                    </div>
                    <div class="oe_snippet_body oe_share">
                        <a class="btn btn-default hover" href="#">Button with hover effect</a>
                    </div>
                </div><!-- End of hover button -->

            </xpath>

            <!-- Customize Features -->
            <xpath expr="//div[@id='snippet_feature']" position="inside">
                <!-- Swedish clock with week day and name of the day -->
                <div id="swedish_clock_snippet">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/snippet_addons/static/src/img/block_clock.png"/>
                        <span class="oe_snippet_thumbnail_title">Swedish Clock</span>
                    </div>
                    <section class="oe_snippet_body jumbotron">
                        <div class="container">
                            <div class="row">
                                <div class="text-center">
                                    <p><font size="3" id="sw_clock">
                                        <span id="clock"/>
                                    </font></p>
                                </div>
                            </div>
                        </div>
                    </section>
                </div><!-- End of Swedish clock with week day and name of the day -->

                <div id="image_gallery_with_hover_text">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/website/static/src/img/blocks/block_image_gallery.png"/>
                        <span class="oe_snippet_thumbnail_title">Image Gallery With Hover Text</span>
                    </div>
                    <section class="oe_snippet_body">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-6" style="padding-left: 0; padding-right: 0;">
                                    <div class="text-center hover-image">
                                        <img src="/website/static/src/img/desert.jpg" alt="" class="img-responsive"/>
                                        <a href="/">
                                            <div class="hover-text hidden">
                                                <p>Hover Text</p>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-md-3" style="padding-left: 0; padding-right: 0;">
                                    <div class="text-center hover-image">
                                        <img src="/website/static/src/img/china_thumb.jpg" alt="" class="img-responsive"/>
                                        <a href="/">
                                            <div class="hover-text hidden">
                                                <p style="left: 30%;">Hover Text</p>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-md-3" style="padding-left: 0; padding-right: 0;">
                                    <div class="text-center hover-image">
                                        <img src="/website/static/src/img/deers_thumb.jpg" alt="" class="img-responsive"/>
                                        <a href="/">
                                            <div class="hover-text hidden">
                                                <p style="left: 30%;">Hover Text</p>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-md-3" style="padding-left: 0; padding-right: 0;">
                                    <div class="text-center hover-image">
                                        <img src="/website/static/src/img/desert_thumb.jpg" alt="" class="img-responsive"/>
                                        <a href="/">
                                            <div class="hover-text hidden">
                                                <p style="left: 30%;">Hover Text</p>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-md-3" style="padding-left: 0; padding-right: 0;">
                                    <div class="text-center hover-image">
                                        <img src="/website/static/src/img/china_thumb.jpg" alt="" class="img-responsive"/>
                                        <a href="/">
                                            <div class="hover-text hidden">
                                                <p style="left: 30%;">Hover Text</p>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>

            </xpath>

        </template>

        <!-- Snippet options -->
        <template id="more_snippet_options" inherit_id="website.snippet_options" name="More Snippet Options">

            <xpath expr="//div[@data-snippet-option-id='transform']" position="after">
                <!-- Hover button options -->
                <div data-snippet-option-id='hover_button' data-selector="a, .btn, .btn-default, .hover">
                    <li class="dropdown-submenu">
                        <!-- 2D Transitions -->
                        <a tabindex="-1" href="#">2D Transitions</a>
                        <ul class="dropdown-menu">
