            /* MENU */
            .bl2-navbar {
                font-family: "Indie Flower";
                font-size: 1.2em;
                background: none;
                width: 100%;
            }

            #bl2-menu-bar &gt; div &gt; ul &gt; li &gt; a {
                padding-top: 25px;
                padding-bottom: 25px;
            }

            #bl2-menu-bar &gt; div {
                width: 1024px;
                margin: auto;
            }

            .bl2-navbar-fixed-top {
                transition: all 0.5s ease-in 0s;
                font-family: "Indie Flower";
                font-size: 1.2em;
                z-index: 99;
                top: 0px;
                position: fixed;
                width: 100%;
                background: #fff;
