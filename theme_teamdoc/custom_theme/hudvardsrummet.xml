<?xml version="1.0"?>
<data inherit_id="website.layout" customize_show="True" active="False" name="Theme Block Logo" priority="99">
    <!-- Customize CSS -->
    <xpath expr="//head/t[@name='layout_head']" position="after">
        <link href="/theme_block_logo/static/src/css/theme_block_logo.css" rel="stylesheet"/>
        <link href='https://fonts.googleapis.com/css?family=Philosopher' rel='stylesheet' type='text/css'/>

        <style>

            .bl-body {
                background: #fff;
                list-style: none;
                font-family: 'Philosopher', sans-serif;
            }

            h1, h2, h3 {
                color: #B9A55A;
                font-family: 'Philosopher', sans-serif;
            }

            h4, h5 {
                color: #000;
            }

            #wrap section, #wrap div, .carousel {
                max-width: 1140px;
                margin: auto;
            }

            /* MENU */
            .bl-navbar {
                font-size: 1.2em;
                background: none;
                width: 100%;
                margin-left: auto;
                margin-right: auto;
            }

            #bl-menu-bar > ul > li > a {
                padding-top: 25px;
                padding-bottom: 25px;
            }

            .bl-navbar-fixed-top {
                font-size: 1.2em;
                z-index: 99;
                top: 0;
                position: fixed;
                width: 100%;
                background: #EDE7D3;
                box-shadow: 0px 0px 20px #B9A55A;
                transition: all 0.3s ease-out;
            }

            #top_menu {
                max-width: 1140px;
                margin: auto;
            }

            #top_menu > .dropdown > .dropdown-menu {
                margin: 0px;
                border-radius: 0px;
                padding: 0px;
            }

            #top_menu > .dropdown > .dropdown-menu span {
                font-size: 1.2em;
            }

            #top_menu > .dropdown > .dropdown-menu > li > a:hover,
            #top_menu > .dropdown > .dropdown-menu > .active > a,
            #top_menu > .dropdown > .dropdown-menu > .active > a:focus,
            #top_menu > .dropdown >.dropdown-menu > .active > a:hover {
                color: #B9A55A;
                background-color: #EDE7D3;
            }

            /* COMPANY LOGO */
            #logo-box {
                padding: 0px;
            }

            #bl-logo {
                max-width: 1140px;
                width: 100%;
                display: block;
                margin: auto;
            }

            /* MENU TEXT */
            #bl-menu-bar > ul > li > a{
                color: #343434;
                padding-left: 0px;
                padding-right: 0px;
            }

            /* STYLE WHEN MOUSE HOVER */
            #bl-menu-bar > ul > li > a:hover,
            #bl-menu-bar > ul > li > a:focus {
                color: #B9A55A;
                background-color: #EDE7D3;
            }

            /* STYLE WHEN PAGE ACTIVE */
            #bl-menu-bar > div > ul > .active > a {
                color: #B9A55A;
                background-color: #EDE7D3;
            }

            #bl-menu-bar {
                width: 100%;
                padding: 0px;
            }

            #bl-menu-bar > .row {
                width: 100%;
            }

            .block {
                padding-left: 0px;
                padding-right: 0px;
            }

            /* FOOTER STYLE*/
            .bl-footer {
                max-width: 2000px;
                background-color: #EDE7D3;
                box-shadow: 0px 0px 20px #B9A55A;
            }

            .bl-footer a {
                color: #B9A55A;
            }

            .round-icon {
                border-color: rgba(198, 159, 86, 0);
                background-color: transparent;
                border-radius: 100px;
                width: 200px;
                height: 200px;
                margin: auto;
                overflow: hidden;
                border-style: solid;
                border-width: 1px;
            }

            .round-icon i {
                padding-top: 33%;
                width: 200px;
                height: 200px;
                color: #C69F56;
            }

            .round-icon:hover {
                border-color: rgba(132, 86, 0, 1);
                background-color: transparent;
                transition: all 0.7s ease-out;
            }

            .round-icon i:hover {
                color: #845600;
                transition: all 0.7s ease-out;
            }

            .hover h3 {
                transition: all 0.7s ease-in;
                color: #fff !important;
                width: 100%;
                height: 15%;
                bottom: 15%;
                line-height: 180%;
                text-align: center;
                position: absolute;
                margin: 0;
                background-color: rgba(0, 0, 0, 0.7);
                opacity: 1;
            }

            .hover:hover h3 {
                opacity: 0.2;
            }

            @media screen and (max-width: 480px) {
                #bl-menu-bar > ul > li > a {
                    padding: 5px 0px 5px 0px;
                }
                section .s_title > div .container {
                    margin-top: 0px;
                }
            }

            div#products_grid_before a {
                background-color: #FFFFFF;
                color: #776337;
            }

            div#products_grid_before > ul > li > a {
                /* background: linear-gradient(to bottom right, #f5d9a7 , #b09c77); */
                margin: 5px 0px 5px 0px;
                border-style: solid;
                border-width: 1px;
                border-color: #C69F56;
            }

            div#products_grid_before li.active > a {
                /* font-weight: bold; */
                background-color: #C69F56;
                color: #fff;
            }
            .ribbon.btn.btn-danger {
                font-size: 12px;
            }

            .oe_product-custom-color-0 section{
                background-color: #EFE8D7;
                color: #000;
            }

            .oe_product-custom-color-0 section a {
                color: #000;
            }

            .oe_product-custom-color-2 section{
                background-color: #EFE8D7;
                color: #000;
            }

            .oe_product-custom-color-2 section a{
                color: #000;
            }

            .oe_product-custom-color-1 section{
                background-color: #EFE8D7;
                color: #000;
            }

            .oe_product-custom-color-1 section a{
                color: #000;
            }

            .oe_product-custom-color-0 a.btn.fa-shopping-cart {
                background-color: #fff;
            }

            .oe_product-custom-color-1 a.btn.fa-shopping-cart {
                background-color: #fff;
            }

            .oe_product-custom-color-2 a.btn.fa-shopping-cart {
                background-color: #fff;
            }

            .oe_product-custom-color-3 a.btn.fa-shopping-cart {
                background-color: #fff;
            }

        </style>
    </xpath>

    <!-- add company logo -->
    <xpath expr="//body/div[@id='wrapwrap']/header" position="before">
        <div class="container">
            <div id="logo-box" class="col-md-12 hidden-xs">
                <img id="bl-logo" src="/theme_teamdoc/static/src/img/hudvardsrummet/header-logo.jpg" t-att-alt="'Logo of %s' % res_company.name" t-att-title="res_company.name"/>
            </div>
        </div>
    </xpath>

    <!-- define id and class to menu bar -->
    <xpath expr="//body/div[@id='wrapwrap']/header/div" position="attributes">
        <attribute name="id">bl-menu</attribute>
        <attribute name="class">bl-navbar</attribute>
    </xpath>

    <!-- define class to menu style -->
    <xpath expr="//header//ul[@id='top_menu']" position="attributes">
        <attribute name="class">nav nav-justified</attribute>
    </xpath>
    <xpath expr="//body/div[@id='wrapwrap']/header/div/div/div[2]" position="attributes">
        <attribute name="id">bl-menu-bar</attribute>
        <attribute name="class">collapse navbar-collapse navbar-top-collapse col-md-12</attribute>
    </xpath>

    <!-- hide company name -->
    <xpath expr="//body/div[@id='wrapwrap']/header/div/div/div/a" position="attributes">
        <attribute name="class">navbar-brand hidden</attribute>
    </xpath>

    <!-- show company logo in mobile version -->
    <xpath expr="//body/div[@id='wrapwrap']/header/div/div/div/button" position="before">
        <img class="pull-left hidden-lg hidden-md hidden-sm" style="max-height: 50px;" src="/logo.png"/>
    </xpath>

    <!-- hide user login / out -->
    <xpath expr="//header//ul[@id='top_menu']/li[@class='divider']" position="attributes">
        <attribute name="class">hidden</attribute>
    </xpath>
    <xpath expr="//header//ul[@id='top_menu']/li[@class='dropdown']" position="attributes">
        <attribute name="class">hidden</attribute>
    </xpath>

    <!-- define a class to body -->
    <xpath expr="//body" position="attributes">
        <attribute name="class">bl-body</attribute>
    </xpath>
    <!-- define a class to footer -->
    <xpath expr="//footer" position="attributes">
        <attribute name="class">bl-footer</attribute>
    </xpath>
    <!-- add log in/out in footer -->
    <xpath expr="//footer" position="inside">
        <div class="container">
            <div class="pull-right">
                <li class="dropdown" t-ignore="true" t-if="website.user_id != user_id">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <b>
                            <span t-esc="user_id.name"/>
                            <span class="caret"/>
                        </b>
                    </a>
                    <ul class="dropdown-menu js_usermenu" role="menu">
                        <li><a href="/web" role="menuitem">My Account</a></li>
                        <li class="divider"/>
                        <li><a t-attf-href="/web/session/logout?redirect=/" role="menuitem">Logout</a></li>
                    </ul>
                </li>
            </div>
            <div class="pull-right">
                <li groups="base.group_public">
                    <a t-attf-href="/web/login">
                        <b>Sign in</b>
                    </a>
                </li>
            </div>
        </div>
    </xpath>
    <!-- Customize javascript -->
    <xpath expr="//body" position="inside">
        <script>
            var margin_value = 178
        </script>
        <script type="text/javascript" src="/theme_block_logo/static/src/js/theme_block_logo.js"/>
    </xpath>
</data>

